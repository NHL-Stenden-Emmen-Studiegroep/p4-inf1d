{
  "expo": {
    "name": "Cubic Calender App",
    "slug": "Apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  },
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./.build",
    "rootDir": "./src",
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "paths": {
      "@calendar/navigation": [
        "./src/navigation"
      ],
      "@calendar/screens": [
        "./src/screens"
      ],
      "@calendar/store": [
        "./src/store"
      ],
      "@calendar/components": [
        "./src/components"
      ],
      "@calendar/hooks": [
        "./src/hooks"
      ]
    }
  },
  "types": [
    "react",
    "react-native"
  ],
  "exclude": ["App.tsx", "babel.config.js", "jest.config.js", ".eslintrc.js", "./bin/**/**"],
  "extends": "expo/tsconfig.base"
}
